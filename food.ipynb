
import pandas as pd
import json
import sqlite3

orders = pd.read_csv("orders.csv")
orders.head()

with open("users.json", "r") as file:
    users_data = json.load(file)

users = pd.DataFrame(users_data)
users.head()

conn = sqlite3.connect(":memory:")

with open("restaurants.sql", "r") as file:
    conn.executescript(file.read())

restaurants = pd.read_sql("SELECT * FROM restaurants", conn)
restaurants.head()

orders_users = orders.merge(
    users,
    on="user_id",
    how="left"
)

orders_users.head()

final_df = orders_users.merge(
    restaurants,
    on="restaurant_id",
    how="left"
)

final_df.head()

final_df.to_csv("final_food_delivery_data.csv", index=False)
print("final_food_delivery_data.csv created successfully")

print("Total rows:", final_df.shape[0])
print("Total columns:", final_df.shape[1])

final_df.info()


